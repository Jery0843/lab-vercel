{
  "name": "0xjerrys-lab",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:wrangler": "npm run build && npx wrangler pages dev .next/standalone --compatibility-date=2024-01-15 --d1=D1_DATABASE=jerrys-lab",
    "dev:cf": "npx wrangler dev --port 3000 --local --persist-to .wrangler/state",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:migrate": "npx wrangler d1 execute jerrys-lab --file=database/schema.sql",
    "db:migrate:remote": "npx wrangler d1 execute jerrys-lab --file=database/schema.sql --remote",
    "db:shell": "npx wrangler d1 execute jerrys-lab --command",
    "trigger:dev": "npx trigger.dev@latest dev",
    "membership:check": "node scripts/membership-cron.js",
    "db:add-expiry": "npx wrangler d1 execute jerrys-lab --file=database/add_expiry_date.sql"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.943.0",
    "@trigger.dev/sdk": "4.2.0",
    "@types/node-fetch": "^2.6.13",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@upstash/redis": "^1.35.7",
    "axios": "^1.11.0",
    "cheerio": "^1.1.2",
    "dotenv": "^17.2.3",
    "framer-motion": "^12.23.22",
    "gray-matter": "^4.0.3",
    "maxelpay-js": "^1.0.1",
    "next": "15.5.7",
    "node-fetch": "^3.3.2",
    "puppeteer": "^24.15.0",
    "react": "19.1.2",
    "react-dom": "19.1.2",
    "react-icons": "^5.5.0",
    "react-markdown": "^10.1.0",
    "react-syntax-highlighter": "^16.1.0",
    "redis": "^5.10.0",
    "remark-breaks": "^4.0.0",
    "remark-gfm": "^4.0.1",
    "resend": "^4.0.1",
    "rss-parser": "^3.13.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^20",
    "@types/puppeteer": "^7.0.4",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "better-sqlite3": "^12.2.0",
    "eslint": "^9",
    "eslint-config-next": "15.4.5",
    "tailwindcss": "^4",
    "typescript": "^5",
    "wrangler": "^4.26.1"
  }
}
